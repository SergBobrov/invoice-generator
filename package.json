{
  "name": "invoice-generator",
  "version": "1.0.0",
  "description": "mern stack",
  "main": "app.js",
  "scripts": {
    "start": "babel-node app.js",
    "server": "nodemon --exec babel-node app.js",
    "client": "npm run start --prefix client",
    "pdfgen": "babel-node pdf_generation/generatePdfWorker.js",
    "pdfsend": "babel-node pdf_mailing/sendPdfWorker.js",
    "workers": "concurrently \"npm run pdfgen\" \"npm run pdfsend\" \"npm run server\"",
    "worker1": "concurrently \"npm run pdfsend\" \"npm run server\""
  },
  "keywords": [
    "node.js",
    "express",
    "react",
    "mongoDB"
  ],
  "author": "Sergei Bobrov <serggbobrov@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bullmq": "^1.12.1",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-validator": "^6.8.0",
    "html-pdf": "^2.2.0",
    "ioredis": "^4.19.4",
    "mailgun-js": "^0.22.0",
    "mongoose": "^5.11.8",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@babel/node": "^7.12.10",
    "@babel/core": "^7.12.10",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
